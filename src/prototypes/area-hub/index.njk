{% block pageContent %}
<main id="main" role="main" tabindex="-1">
    <section class="ons-hero ons-grid--gutterless">
        <div class="ons-hero__container ons-container">
            <div class="ons-hero__details ons-grid__col ons-col-8@m ons-col-10@s@m">
                 {{
    onsBreadcrumbs({
        "ariaLabel": 'Breadcrumbs',
        "itemsList": [
            {
                "url": '/',
                "text": 'Home'
            },
            {
                "url": '/',
                "text": 'Local statistics'
            }
        ]
    })
}}
            <header>
                
                <h1 class="ons-hero__title ons-u-fs-xxxl">Explore local statistics</h1>
            </header>
            <p class="ons-hero__text">Find, visualise, compare and download statistics for areas within the United Kingdom.</p>
             {{ onsAutosuggest({
            "id": "country-of-birth-autosuggest",
            "input": {
                "label": {
                    "text": "Search by area name or postcodse",
                    "description": "Enter your own answer or select from suggestions",
                    "id": "country-of-birth-label"
                },
                "autocomplete": "off"
            },
            "instructions": "Use up and down keys to navigate suggestions once you\'ve typed more than three characters. Use the enter key to select a suggestion. Touch device users, explore by touch or with swipe gestures.",
            "ariaYouHaveSelected": "You have selected",
            "ariaMinChars": "Enter 3 or more characters for suggestions.",
            "ariaOneResult": "There is one suggestion available.",
            "ariaNResults": "There are {n} suggestions available.",
            "ariaLimitedResults": "Results have been limited to 10 suggestions. Type more characters to improve your search",
            "moreResults": "Continue entering to improve suggestions",
            "resultsTitle": "Suggestions",
            "resultsTitleId": "area-name-suggestions",
            "autosuggestData": "https://raw.githubusercontent.com/Joe-Dixon-ID/website-prototypes/main/src/prototypes/area-name-suggestions.json",
            "noResults": "No suggestions found. You can enter your own answer",
            "typeMore": "Continue entering to get suggestions"
        }) }}

            </div>
        </div>
    </section>



<div class="ons-grid__col ons-grid__col--sticky@m ons-col-4@m">
</main>
{% endblock %}



